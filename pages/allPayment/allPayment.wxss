/* pages/allPayment/allPayment.wxss */
.order{
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  border-bottom: 1px solid #ccc;
}
.order view{
  width: 33.333333%;
  height: 100%;
  float: left;
  text-align: center;
}
.order view text{
  font-size: 16px;
  color: #909090;
  display: inline-block;
  height: 100%;
}
.order view .act{
  font-size: 16px;
  color: #383838;
  border-bottom: 1px solid #383838;
}
.pay{
  width: 100%;

}
.pay-con{
  width: 670rpx;
  margin: 42rpx auto 0;
  
}
.ding-main{
  width: 630rpx;
  padding: 0 20rpx;
  margin: 0 auto 30rpx;
  background: #f6f6f6;
}
.ding-main1{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 1px solid #e6e6e6;
}
.ding-num{
  color: #353535;
  font-size: 14px;
  float: left;
}
.ding-status{
  float: right;
  height: 100%;
  color: #fb5858;
  font-size: 14px;
  background: url(http://b247.photo.store.qq.com/psb?/V10RQdNV3tmS94/RzA0R8k.nRvC43NyU5zPjsMg6*thjPWB1ql8.61zNnA!/m/dEQAAAAAAAAAnull&bo=CwAUAAAAAAADBz0!&rf=photolist&t=5) right center no-repeat;
  background-size: 14rpx 22rpx;
  padding-right: 10px;
}
.ding-main2{
  width: 100%;
  border-bottom: 1px solid #e6e6e6;
  padding-bottom: 10px;
}
.ding-main2 view:nth-child(1) {
  margin-top: 10px;
}
.ding-main2 view{
  color: #353535;
  font-size: 14px;
  line-height: 50rpx;
}
.ding-money{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  margin-top: 10px;
  font-size: 14px;
  padding-bottom: 10px;
}
.ding-money text{
  color: #f54646;
}
.zhifu{
  float: left;
}
.tian{
  display: inline-block;
  width: 130rpx;
  height: 100%;
  float: right;
  background: #5989d8;
  color: #fff;
  text-align: center;
  border-radius: 5px;
  padding: 0 5px;
}



