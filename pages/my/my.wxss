page{
  background: #f5f5f5;
}
.my-list{
  padding-bottom: 100rpx;
}
.tab{
  width: 100%;
  height: 44px;
  position: fixed;
  bottom:0;
  left:0;
  border-top: 1px solid #e2e4e3;
  background: #fff;
  z-index: 999999;
}
.tab cover-image{
  width: 22px;
  height: 22px;
  margin-top:6px;
  display: block;
}
.tab-view1,.tab-view2,.tab-view3{
  text-align: center;
  width: 33.333333%;
  height: 100%;
  float: left;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}
.index-view,.my-view{
  font-size: 12px;
}
.index-view{
  color:#c0c0c0;
}
.my-view{
  color:#383838;
}
.my{
  width: 100%;
  background: #fff;
  margin-bottom: 10px;
}
.my-main{
  width: 670rpx;
  margin: 0 auto;
}
.ding{
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 1px solid #e6e6e6;
}
.ding-con{
  width: 670rpx;
  height: 100%;
  margin: 0 auto;
}
.ding-title{
  float: left;
  color: #111111;
  font-size: 16px;
}
.ding-cha{
  float: right;
  color: #6f6f6f;
  font-size: 12px;
  background: url(http://b247.photo.store.qq.com/psb?/V10RQdNV3tmS94/RzA0R8k.nRvC43NyU5zPjsMg6*thjPWB1ql8.61zNnA!/m/dEQAAAAAAAAAnull&bo=CwAUAAAAAAADBz0!&rf=photolist&t=5) right center no-repeat;
  background-size: 14rpx 22rpx;
  padding-right: 10px;
}
.ding-main{
  width: 630rpx;
  margin: 0 auto;
}
.ding-main1{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 1px solid #e6e6e6;
}
.ding-num{
  color: #353535;
  font-size: 14px;
  float: left;
}
.ding-status{
  float: right;
  height: 100%;
  color: #fb5858;
  font-size: 14px;
  background: url(http://b247.photo.store.qq.com/psb?/V10RQdNV3tmS94/RzA0R8k.nRvC43NyU5zPjsMg6*thjPWB1ql8.61zNnA!/m/dEQAAAAAAAAAnull&bo=CwAUAAAAAAADBz0!&rf=photolist&t=5) right center no-repeat;
  background-size: 14rpx 22rpx;
  padding-right: 10px;
}
.ding-main2{
  width: 100%;
  border-bottom: 1px solid #e6e6e6;
  padding-bottom: 10px;
}
.ding-main2 view:nth-child(1) {
  margin-top: 10px;
}
.ding-main2 view{
  color: #353535;
  font-size: 14px;
  line-height: 50rpx;
}
.ding-money{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  margin-top: 10px;
  font-size: 14px;
  padding-bottom: 10px;
}
.ding-money text{
  color: #f54646;
}
.zhifu{
  float: left;
}
.tian{
  display: inline-block;
  width: 130rpx;
  height: 100%;
  float: right;
  background: #5989d8;
  color: #fff;
  text-align: center;
  border-radius: 5px;
  padding: 0 5px;
}
.qihao{
  color: #111111;
  font-size: 14px;
  line-height: 80rpx;
}
.nei{
  width: 100%;
  height: 285rpx;
  padding-bottom:10px; 
}
.nei2{
  width: 100%;
  height: 250rpx;
  padding-bottom:10px; 
}
.nei-item{
  width: 315rpx;
  height: 100%;
  float: left;
}
.nei-item:nth-child(1) {
  margin-right: 39rpx;
}
.film-image{
  width: 100%;
  height: 184rpx;
  float: left;
  position: relative;
}
.film-yin{
  background:#fff;
  width: 100%;
  height: 184rpx;
  float: left;
  position: relative;
}
.film-image{
  background:#000;
}
.film-image .yinImg,.film-yin .yinImg{
  width: 100%;
  height: 100%;
}
.film-image .shiImg,.film-yin .shiImg{
  margin: 0 20%;
  width: 60%;
  height: 100%;
  border-radius: 0px;
}
.film-name{
    margin: 10rpx 0;
    font-size: 16px;
    color:#353535;
    overflow: hidden;
text-overflow:ellipsis;
white-space: nowrap;
}
.film-num{
  float: left;
  font-size: 14px;
  color:#868686;
   padding-left:14%;
   background: url(http://b247.photo.store.qq.com/psb?/V10RQdNV3tmS94/jfv9.a1OydLfux.MZ5eg7mchqS3nfe993dbYDK8bqo8!/m/dEQAAAAAAAAAnull&bo=EgAPAAAAAAADBz8!&rf=photolist&t=5) left center no-repeat;  
}
.shichang{
  float: right;
  color: #868686;
  font-size: 12px;
  padding-left:14%;
    background: url(http://b247.photo.store.qq.com/psb?/V10RQdNV3tmS94/1sRkWXcsuEi87bqSYKBiLrMNcJw4e3*VC4iSJJNL19M!/m/dDEBAAAAAAAAnull&bo=FAAUAAAAAAADByI!&rf=photolist&t=5) left center no-repeat;  
    background-size: 18px 18px;
}
.film-info{
  width: 100%;
  height: 106rpx;
  float:right;
}
.start-ping{
  height: 40rpx;
  line-height: 40rpx;
  float: left;
  margin-top: 5px;
}
.start-ping text{
  float: left;
  font-size: 14px;
  margin-right: 10px;
}
image{
  display: inline-block;
  width: 52rpx;
  height: 52rpx;
}
.start{
  float: left;
  width: 74px;
  height: 20px;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}
.icon{
  background: url(http://b247.photo.store.qq.com/psb?/V10RQdNV3tmS94/JnujFnBqCw72zbBC*A1MVx19RK04ksHU7RvqWDYPQ.M!/m/dPcAAAAAAAAAnull&bo=KAAoAAAAAAADByI!&rf=photolist&t=5) center center no-repeat;
  width: 52rpx;
  height: 32rpx;
  background-size: 100% 100%;
  margin-right:2px; 
}
.cur{
  background: url(http://b247.photo.store.qq.com/psb?/V10RQdNV3tmS94/zoqZpDmXvhbCfi5uQ0Z8Ysyn.f4Bq3WMLfZpEJutQxs!/m/dLMAAAAAAAAAnull&bo=KAAoAAAAAAADByI!&rf=photolist&t=5) center center no-repeat;
  width: 52rpx;
  height: 32rpx;
  background-size: 100% 100%;
}
.qing{
  width: 100%;
  line-height: 100rpx;
  height: 100rpx;
  text-align: center;
  font-size: 18px;
}
.qing text{
  color: #72c365;
}
